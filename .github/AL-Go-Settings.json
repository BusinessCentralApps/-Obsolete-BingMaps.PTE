{
  "type": "PTE",
  "templateUrl": "https://github.com/freddydk/AL-Go-PTE@linux",
  "currentSchedule": "0 2 * * 1,2,3,4,5",
  "NextMinorSchedule": "0 2 * * 6",
  "NextMajorSchedule": "0 2 * * 0",
  "useProjectDependencies": true,
  "runs-on": "ubuntu-latest",
  "shell": "pwsh",
  "githubRunner": "ubuntu-latest",
  "githubRunnerShell": "pwsh",
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "doNotSignApps": true,
  "vsixFile": "https://bcartifacts.blob.core.windows.net/prerequisites/al-12.0.0-marketplace.vsix",
  "UpdateGitHubGoSystemFilesSchedule": "0 0 * * *",
  "updateDependencies": true,
  "cacheImageName": "",
  "DoNotRunBCPTTests": true,
  "assignPremiumPlan": true,
  "DeployToQA": {
    "Branches": [
      "UAT",
      "main"
    ],
    "EnvironmentName": "QA",
    "Projects": "*"
  },
  "DeliverToTeams": {
    "Branches": [
      "UAT",
      "main"
    ]
  },
  "defaultNewContainerParameters": {
    "dns": "hostDNS"
  },
  "ConditionalSettings": [
    {
      "branches": [
        "feature/*"
      ],
      "settings": {
        "doNotPublishApps": true,
        "doNotSignApps": true
      }
    }
  ]
}
